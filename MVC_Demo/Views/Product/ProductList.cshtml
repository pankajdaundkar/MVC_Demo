@{
    ViewData["Title"] = "ProductList";
}

<h1>Index</h1>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Price</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Products p in (IEnumerable<Products>)ViewData["list"])
        {
            <tr>
                <td>@p.Id</td>
                <td>@p.Name</td>
                <td>@p.Price</td>
            </tr>

        }
    </tbody>
</table>
